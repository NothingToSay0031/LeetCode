{
    "C_Cpp.clang_format_fallbackStyle": "Google",
    "C_Cpp.clang_format_path": "/usr/bin/clang-format-14",
    "C_Cpp.default.enableConfigurationSquiggles": true,
    "C_Cpp.errorSquiggles": "disabled",
    "C_Cpp.codeAnalysis.runAutomatically": false,
    "C_Cpp.codeAnalysis.clangTidy.checks.disabled": [
        "clang-analyzer-*"
    ],
    "markdown.preview.fontSize": 16,
    "markdown.preview.fontFamily": "'思源宋体', 'Fira Code', '思源黑体'",
    "markdown.preview.lineHeight": 1.4,
    "markdown.occurrencesHighlight.enabled": true,
    "markdown.preview.breaks": false,
    "markdown.preview.typographer": false,
    "markdown.preview.linkify": false,
    "markdown.preview.markEditorSelection": false,
    "editor.tabSize": 2,
}